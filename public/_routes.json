{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/_next/*", "/_static/*"],
  "rules": [
    {
      "type": "rewrite",
      "include": ["/*"],
      "exclude": ["/_next/*", "/_static/*", ".html", ".js", ".css", ".png", ".jpg", ".svg"],
      "paths": {
        "/*": "/index.html"
      }
    }
  ]
}
